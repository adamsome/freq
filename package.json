{
  "name": "freq",
  "author": "@adamsome",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "cookie": "^0.4.1",
    "mongodb": "^3.5.9",
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.0",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
